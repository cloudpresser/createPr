{
  "name": "azure-devops-pr-creator",
  "version": "1.0.0",
  "description": "A TypeScript library to create pull requests in Azure DevOps",
  "main": "dist/azureDevOpsClient.js",
  "types": "dist/azureDevOpsClient.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "start": "npx ts-node cli.ts"
  },
  "keywords": [
    "azure",
    "devops",
    "pull request",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.4.5",
    "inquirer": "8.2.4",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/inquirer": "8.2.1",
    "@types/jest": "^29.5.13",
    "@types/node": "^18.11.9",
    "@types/yargs": "^17.0.33",
    "jest": "^29.2.2",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.1",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ]
  }
}
